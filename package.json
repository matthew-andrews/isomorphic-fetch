{
  "name": "universal-fetch",
  "version": "0.0.0",
  "description": "Universal WHATWG Fetch API, for Node & Browser with IE8 support",
  "browser": "fetch-npm-browser.js",
  "main": "fetch-npm-node.js",
  "scripts": {
    "files": "find . -name '*.js' ! -path './node_modules/*' ! -path './bower_components/*'",
    "test": "jshint `npm run -s files` && lintspaces -i js-comments -e .editorconfig `npm run -s files` && mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Pitzcarraldo/universal-fetch.git"
  },
  "author": "Minkyu Cho <mrnoname@naver.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pitzcarraldo/universal-fetch/issues"
  },
  "homepage": "https://github.com/Pitzcarraldo/universal-fetch/issues",
  "dependencies": {
    "fetch-ie8": "^1.4.0",
    "node-fetch": "^1.3.3"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "es6-promise": "^2.0.1",
    "jshint": "^2.5.11",
    "lintspaces-cli": "^0.1.1",
    "mocha": "^2.1.0",
    "nock": "^0.56.0",
    "npm-prepublish": "^1.0.2"
  }
}
